{
  "platform": {
    "apiRuntime": "node:18"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/_next/*",
      "/static/*",
      "/public/*",
      "/favicon.ico",
      "/*.{png,jpg,jpeg,gif,svg,css,js,woff2,ttf}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}