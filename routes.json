{
  "platform": {
    "apiRuntime": "node:18"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*",
      "/assets/*",
      "/_next/*",
      "/favicon.ico",
      "/*.{png,jpg,jpeg,gif,svg,css,js,woff2,ttf}"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "mimeTypes": {
    ".json": "text/json",
    ".js": "text/javascript",
    ".html": "text/html"
  }
}